This is the celebrity page

<ul>
    {{#each celebrity}}
    <li>
        <a href="/celebrities/{{this._id}}">
            {{this.name}}
        </a>
        <form action="/celebrities/{{this._id}}/edit" method="get">
            <button>Edit</button>
        </form>
        <form action="/celebrities/{{this._id}}/delete" method="post">
            <button>Delete</button>
        </form>
    </li>
    {{/each}}
</ul>

<a href="/celebrities/newCelebrity">Add a new celebrity</a>