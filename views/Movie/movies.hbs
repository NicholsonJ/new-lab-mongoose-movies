This is the movie page

<ul>
    {{#each movie}}
    <li>
        <a href="/movies/{{this._id}}">
            {{this.title}}
        </a>
        <form action="/movies/{{this._id}}/edit" method="get">
            <button>Edit</button>
        </form>
        <form action="/movies/{{this._id}}/delete" method="post">
            <button>Delete</button>
        </form>
    </li>
    {{/each}}
</ul>

<a href="/movies/newMovie">Add a new movie</a>